<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:id="@+id/principal">
    <fragment
            android:layout_width="411dp"
            android:layout_height="244dp" android:name="moreno.alex.fragmentos.PrimerFragment"
            android:id="@+id/fragment"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" tools:layout="@layout/fragment_primer"/>
    <fragment
            android:layout_width="414dp"
            android:layout_height="250dp" android:name="moreno.alex.fragmentos.SegundoFragment"
            android:id="@+id/fragment2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fragment" app:layout_constraintHorizontal_bias="0.507"
            tools:layout="@layout/fragment_segundo"/>
    <fragment
            android:layout_width="414dp"
            android:layout_height="241dp" android:name="moreno.alex.fragmentos.TercerFragment"
            android:id="@+id/fragment3"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fragment2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" tools:layout="@layout/fragment_tercer"/>
</androidx.constraintlayout.widget.ConstraintLayout>